{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names vite,api --prefix-colors cyan,magenta \"vite\" \"bun --watch run server/index.ts\"",
    "dev:web": "vite",
    "dev:server": "bun --watch run server/index.ts",
    "build": "tsc -b && vite build",
    "start": "NODE_ENV=production bun run server/index.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "bun run server/db/seed.ts",
    "db:studio": "drizzle-kit studio"
  },
